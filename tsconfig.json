{
	"compilerOptions": {
		"incremental": true,
		"target": "es2017",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"jsx": "preserve",
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"],
		"types": ["vite/client"],
		"skipLibCheck": true
	},
	"files": ["src/index.d.ts", "src/vue-shims.d.ts"],
	"include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"]
}
